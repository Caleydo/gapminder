{
  "name": "gapminder",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/gapminder",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_vis": "*",
    "caleydo_d3": "*",
    "caleydo_bootstrap_fontawesome": "*",
    "caleydo_clue": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "view",
          "id": "gapminder",
          "file": "gapminder",
          "location": "gapminder"
        },
        {
          "type": "actionFactory",
          "id": "gapminder",
          "file": "gapminder",
          "factory": "createCmd",
          "creates": "(setGapMinderAttribute|setGapMinderAttributeScale|toggleGapMinderTrails)"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttribute",
          "file": "gapminder",
          "factory": "compressSetAttribute",
          "matches": "setGapMinderAttribute"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttribute",
          "file": "gapminder",
          "factory": "compressToggleGapMinderTrails",
          "matches": "toggleGapMinderTrails"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttributeScale",
          "file": "gapminder",
          "factory": "compressSetAttributeScale",
          "matches": "setGapMinderAttributeScale"
        },
        {
          "type": "app",
          "name": "GapMinder"
        }
      ]
    },
    "products": [
      {
        "name": "GapMinder",
        "type": "python",
        "app": "gapminder"
      }
    ]
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
