{
  "name": "gapminder",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/gapminder",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_provenance": "*",
    "caleydo_vis": "*",
    "caleydo_d3": "*",
    "wrapper_bootstrap_fontawesome": "*",
    "clue_demo": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "view",
          "id": "gapminder",
          "file": "gapminder",
          "location": "gapminder"
        },
        {
          "type": "actionFactory",
          "id": "gapminder",
          "file": "gapminder",
          "factory": "createCmd",
          "creates": "(setGapMinderAttribute|setGapMinderAttributeScale|toggleGapMinderTrails)"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttribute",
          "file": "gapminder",
          "factory": "compressSetAttribute"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttribute",
          "file": "gapminder",
          "factory": "compressToggleGapMinderTrails"
        },
        {
          "type": "actionCompressor",
          "id": "gapminder-setGapMinderAttributeScale",
          "file": "gapminder",
          "factory": "compressSetAttributeScale"
        },
        {
          "type": "app",
          "name": "GapMinder"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3-lasso-plugin": "https://github.com/thinkh/D3-Lasso-Plugin.git"
      }
    },
    "requirejs-config": {
      "paths": {
        "d3-lasso-plugin": "${baseUrl}/d3-lasso-plugin/lasso"
      },
      "shim": {
        "d3-lasso-plugin": [
          "d3"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
